doctype html
html
  head
    link(rel='stylesheet', href='/css/style.css')
    title #{title}
  body


    header
      .module--infos
        h1 Publishing Jockey Machine

    .content-wrapper
      #page-content.page(data-active-block= 1)
        .page-wrapper(data-part="0")
    .page-border

    .infos-wrapper
      .infos-grey
      .infos-box
        h3 Your poster has been generated successfully
        hr
        p And now it should be printing <br> Please take it from the printer


    .js--templates
      div.content.--js-content-el.small-push-1

    script(src='/socket.io/socket.io.js')
    script(src='/bower_components/jquery/dist/jquery.min.js')
    script(src='/bower_components/foundation/js/foundation.min.js')
    script(src='/bower_components/showdown/dist/showdown.min.js')
    script(src='/bower_components/jquery-ui/jquery-ui.min.js')
    script(src='/js/jquery.textfill.js')
    script(src='/settings.js')
    script(src='/js/plugins.js')
    script(src='/js/index.js')


